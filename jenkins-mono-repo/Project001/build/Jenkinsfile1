pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building project 001 using jenkinsfile1..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing project 001..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying project 001....'
            }
        }
    }
}
